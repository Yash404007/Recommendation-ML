      "source": [
        "def director(obj):\n",
        "  L= []\n",
        "  for i in ast.literal_eval(obj):\n",
        "    if i['job'] == 'director' :\n",
        "      L.append(i['name'])\n",
        "    else:\n",
        "      break\n",
        "    return L"
      ],
